{
  "name": "@lazy-panda-ui/lazy-panda-ui",
  "version": "0.1.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "description": "A simple UI framework for React Native (Expo compatible)",
  "keywords": [
    "react-native",
    "expo",
    "ui",
    "components"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "expo": ">=44.0.0",
    "react": ">=17.0.0",
    "react-native": ">=0.64.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.27.1",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-native": "^0.72.8",
    "babel-jest": "^30.0.5",
    "eslint": "^9.32.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "^18.2.0",
    "react-native": "^0.72.14",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc --project tsconfig.json",
    "test": "jest",
    "lint": "eslint ./src --ext .ts,.tsx",
    "format": "prettier --write ./src",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint && npm run build",
    "dist": "npm run test && npm run build && cp -R src dist && find dist -type f -name '*.ts' -delete && find dist -type f -name '*.tsx' -delete"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@react-native-community/slider": "^4.5.7"
  }
}
