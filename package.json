{
  "name": "@lazy-panda-ui/lazy-panda-ui",
  "version": "0.1.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "description": "A simple UI framework for React Native (Expo compatible)",
  "keywords": [
    "react-native",
    "expo",
    "ui",
    "components"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "expo": ">=44.0.0",
    "react": ">=17.0.0",
    "react-native": ">=0.64.0"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.27.1",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.67.0",
    "ajv": "^8.12.0",
    "ajv-keywords": "^5.1.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "metro-react-native-babel-preset": "^0.76.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "^0.68.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc --project tsconfig.json",
    "test": "jest",
    "lint": "eslint ./src --ext .ts,.tsx",
    "format": "prettier --write ./src",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "dist": "npm run test && npm run build && cp -R src dist && find dist -type f -name '*.ts' -delete && find dist -type f -name '*.tsx' -delete"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@react-native-community/slider": "^4.5.7"
  }
}
